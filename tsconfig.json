{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "strictNullChecks": true
    },
    "exclude": ["node_modules"]
}
